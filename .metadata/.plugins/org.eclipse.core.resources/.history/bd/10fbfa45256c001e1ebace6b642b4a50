package com.training.adi;

public class PermanetEmployee extends Employee{

	private double pfPercentage = 12;
	private double pfAmount;
	private double sal;

	public PermanetEmployee(int empId, String empName, String deptName, double salary) {
		super(empId, empName, deptName, salary);
		this.sal = salary;
	}
	
	public double getPfAmount() {
		return 0.12 * sal;
	}
}
